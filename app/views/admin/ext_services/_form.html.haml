= form_for @admin_ext_service do |f|
  - if @admin_ext_service.errors.any?
    #error_explanation
      %h2= "#{pluralize(@admin_ext_service.errors.count, "error")} prohibited this admin_ext_service from being saved:"
      %ul
        - @admin_ext_service.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :url
    = f.text_field :url
  .actions
    = f.submit 'Save'
